{
  "name": "@goovee/orm",
  "version": "0.0.6",
  "author": "Axelor",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "https://github.com/axelor/goovee-orm",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && rslib build",
    "dev": "pnpm build --watch",
    "release": "pnpm publish --no-git-checks",
    "test": "vitest",
    "test:e2e": "vitest --mode e2e --reporter=verbose --test-timeout 10000",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,css,scss,html}\""
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./*": {
        "types": "./dist/*/index.d.ts",
        "import": "./dist/*/index.js",
        "require": "./dist/*/index.cjs"
      }
    },
    "bin": {
      "goovee": "./dist/cli/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "goovee": "./src/cli/index-dev.cjs"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "glob": "^11.0.3",
    "typeorm": "^0.3.27"
  },
  "peerDependencies": {
    "graphql": "^16.8.1",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@rslib/core": "^0.16.1",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.13.5",
    "@types/node": "^24.9.1",
    "@types/pg": "^8.15.5",
    "@types/yargs": "^17.0.33",
    "dayjs": "^1.11.18",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.2"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
