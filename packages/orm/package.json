{
  "name": "@goovee/orm",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "rm -fr dist/* && tsc -p tsconfig.build.json",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf ./dist",
    "test": "vitest",
    "test:e2e": "vitest --mode e2e --no-threads"
  },
  "sideEffects": false,
  "type": "commonjs",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.js",
      "./src/*/index.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    },
    "exports": {
      ".": "./dist/index.js",
      "./*": [
        "./dist/*.js",
        "./dist/*/index.js"
      ]
    },
    "bin": {
      "goovee": "./dist/cli/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "goovee": "./src/cli/index-dev.mjs"
  },
  "dependencies": {
    "graphql": "^16.8.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@swc/core": "^1.5.0",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.5",
    "@types/yargs": "^17.0.32",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.1"
  }
}
